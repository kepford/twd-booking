{
  "parser": "babel-eslint",
  "root": true,
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "camelcase": [
      2
    ],
    "eqeqeq": [
      2
    ],
    "strict": [
      2
    ],
    "no-undef": [
      1
    ],
    "new-cap": [
      2
    ],
    "no-unused-vars": [
      1
    ],
    "one-var": [
      2,
      "never"
    ],
    "brace-style": [
      2,
      "stroustrup"
    ],
    "valid-jsdoc": [
      2
    ],
    "block-scoped-var": [
      1
    ],
    "curly": [
      2
    ],
    "no-alert": [
      1
    ],
    "no-eval": [
      2
    ],
    "no-implicit-coercion": [
      2
    ],
    "no-loop-func": [
      2
    ],
    "no-script-url": [
      2
    ],
    "no-sequences": [
      2
    ],
    "no-useless-concat": [
      2
    ],
    "yoda": [
      2
    ],
    "no-use-before-define": [
      2
    ],
    "eol-last": [
      2
    ],
    "no-trailing-spaces": [
      2
    ],
    "no-underscore-dangle": [
      2
    ],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "require-jsdoc": [
      2
    ],
    "space-before-blocks": [
      2
    ]
  }
}
